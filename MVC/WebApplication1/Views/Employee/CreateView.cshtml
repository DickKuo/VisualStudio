@model  WebApplication1.Models.EmployeeMgrViewModels.EmployeeManamgerViewModel
@{
    ViewBag.Title = "Create";
}

<h2>新增</h2>


@using (Html.BeginForm("Create", "Employee", FormMethod.Post))
{
    <table class="table">
        <tr>
            <td>
                FirstName :
            </td>
            <td>
                @Html.TextBoxFor(M => M.Emp.EmpFirstName)
            </td>
        </tr>
        <tr>
            <td>
                LastName :
            </td>

            <td>
                @Html.TextBoxFor(M => M.Emp.EmpLastName)
            </td>
        </tr>
        <tr>
            <td>
                Tel :
            </td>

            <td>
                @Html.TextBoxFor(M => M.Emp.Tel)
            </td>
        </tr>        
        <tr>
            <td>
                Address :
            </td>

            <td>
                @Html.TextBoxFor(M => M.Emp.Addr)
            </td>
        </tr>
        <tr>
            <td>
                <input type="submit" class="btn btn-default" value="新增" />
            </td>
        </tr>
    </table>
}